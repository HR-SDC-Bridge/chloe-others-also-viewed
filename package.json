{
  "name": "vbao-others-also-viewed",
  "version": "1.0.0",
  "description": "",
  "author": "vbaobao",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "jest": {
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "**/server/*.{js,jsx}",
      "**/server/database/*.{js,jsx}",
      "!**/server/app.js",
      "!**/server/database/database.js",
      "!**/server/database/seed.js"
    ],
    "globals": {
      "window": {}
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "build": "webpack --watch",
    "seed": "node ./server/database/seed.js",
    "seedPG": "node ./server/database/seedPostgres.js",
    "seedCass": "node ./server/database/seedCassandra.js",
    "start": "nodemon ./server/app.js",
    "test": "jest --no-cache -u --collect-coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cassandra-driver": "^4.6.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.3",
    "nodemon": "^2.0.7",
    "pg": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "enzyme": "^3.11.0",
    "eslint-config-hackreactor": "github:reactorcore/eslint-config-hackreactor",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0"
  }
}
